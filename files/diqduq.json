{
    "class":	 	["CLASS", "className", "{", "classVarDec*", "subroutineDec*", "}"],
    "classVarDec":	 	[["STATIC", "FIELD"], "type" , "varName", "subClassVarDec*", ";"],
    "subClassVarDec" : [",", "varName"],
    "type":	 	[["INT" ,"CHAR","BOOLEAN","className"]],
    "subroutineDec":	 	[["CONSTRUCTOR","FUNCTION","METHOD"], ["VOID" ,"type"], "subroutineName", "(", "parameterList?", ")", "subroutineBody"],
    "parameterList":	 	[ "type", "varName", "subparameterList*" ],
    "subparameterList": [",", "type", "varName"],
    "subroutineBody":	 	["{", "varDec*", "statements", "}"],
    "varDec":	 	["VAR", "type", "varName" ,"subvarDec*", ";"],
    "subvarDec":    ["," ,"varName"],
    "className":	 	["identifier"],
    "subroutineName":	 	["identifier"],
    "varName":	 	["identifier"],

    "statements":	 	["statement*"],
    "statement":	 	[["letStatement","ifStatement","whileStatement","doStatement","returnStatement"]],
    "letStatement":		["LET", "varName", "subletStatement?", "=", "expression", ";"],
    "subletStatement":  ["[", "expression", "]"],
    "ifStatement":	 	["IF", "(", "expression", ")" ,"{", "statements" , "}" , "elseifStatement?"],
    "elseifStatement":  ["ELSE", "{", "statements" ,"}"],
    "whileStatement":	 ["WHILE", "(", "expression", ")", "{" ,"statements", "}"],
    "doStatement":	 	["DO", "subroutineCall", ";"],
    "returnStatement":	 	["RETURN", "expression?", ";"],

    "expression":	 	["term", "subexpression*"],
    "subexpression":    ["op", "term"],
    "term":	 	[["integerConstant","stringConstant","keywordConstant","varName","varName", "subtemp1" ,"subroutineCall", "subterm2" , "subterm3"]],
    "subtemp1": ["[" ,"expression", "]"],
    "subterm2": ["(", "expression", ")"],
    "subterm3": [ "unaryOp", "term"],
    "subroutineCall":	 	[["subsubroutineCall1","subsubroutineCall2"]],
    "subsubroutineCall1":    ["subroutineName", "(", "expressionList", ")"],
    "subsubroutineCall2":   [["className","varName"] ,".", "subroutineName", "(" ,"expressionList", ")"],
    "expressionList":	 	["subexpressionList?"],
    "subexpressionList":    ["expression", "subsubexpressionList*"],
    "subsubexpressionList": [",", "expression"],
    "op":	 	[["+","-","*","/","&","|","<", ">","="]],
    "unaryOp":	 	[["-","~"]],
    "keywordConstant":	 	[["true","false","null" ,"this"]]
}